<div class="profile-box">
    <div class="card">
            <div class="card-content">
              <div class="media">
                <div class="media-left">
                  <figure class="image is-96x96">
                    <img src="https://bulma.io/images/placeholders/96x96.png" alt="Placeholder image">
                  </figure>
                </div>
                <div class="media-content">
                  <p class="title is-4"><%=user.name%></p>
                  <p class="subtitle is-6">@<%=user.username%></p>
                  <p class="subtitle is-6">Member Since: <%=user.memberSince.substring(0,10)%></p>
                </div>
              </div>
          
              <div class="content">
                <div class="biography">
                        Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                        Phasellus nec iaculis mauris. <a>@bulmaio</a>.
                        <a href="#">#css</a> <a href="#">#responsive</a> 
                </div>
              </div>
            </div>
          </div>
          <% var lastPage = currentPage - 1 %>
          <% var reversedFweets = user.fweets.reverse() %>
          <% for (var currentFweet = lastPage * 10; currentFweet < currentPage * 10; currentFweet++) { %>          
            <% if (currentFweet < user.fweets.length) {%>
              <h2 class="title is-2"><%=reversedFweets[currentFweet]['subject']%></h2>
              <%}%>
          <% } %>
</div>